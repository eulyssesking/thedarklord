_G.Thedarklord = true
while wait() do
    if _G.Thedarklord == false then
        return
end
if _G.Thedarklord == true then
    local Event = game:GetService("ReplicatedStorage").Remotes.Bench:FireServer()
    local Event = game:GetService("ReplicatedStorage").Remotes.Bench:FireServer()
    local Event = game:GetService("ReplicatedStorage").Remotes.Bench:FireServer()
end
end
